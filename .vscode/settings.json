{
	"[typescript]": {
		"editor.defaultFormatter": "dbaeumer.vscode-eslint",
		"editor.formatOnSave": true
	},
	"[typescriptreact]": {
		"editor.defaultFormatter": "dbaeumer.vscode-eslint",
		"editor.formatOnSave": true
	},
	"eslint.run": "onType",
	"eslint.format.enable": true,
	"typescript.tsdk": "node_modules/typescript/lib",
	"cSpell.ignoreWords": ["fmod", "spideyy"],
	"Lua.diagnostics.disable": ["missing-module-return"],
	"Lua.diagnostics.globals": ["remodel"],
	"powermode.enabled": true,
	"roblox-ts.color-picker.precision": 3,
	"roblox-ts.command.parameters": ["-w", "--verbose"],
	"roblox-ts.command.development": false,
	"gruvboxMaterial.darkContrast": "hard",
	"gruvboxMaterial.darkPalette": "material"
}
